{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "error",
        "semi": "error",
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "quotes": ["warn", "single"],
        "no-duplicate-imports": "error",
        "no-var": "error",
        "arrow-spacing": "error",
        "no-unused-vars": ["warn"]
    }
}
